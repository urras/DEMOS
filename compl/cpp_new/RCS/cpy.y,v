head     1.3;
access   ;
symbols  ;
locks    ;
comment  @ * @;


1.3
date     87.01.11.16.22.36;  author avg;  state Exp;
branches ;
next     1.2;

1.2
date     87.01.11.16.04.46;  author avg;  state Exp;
branches ;
next     1.1;

1.1
date     87.01.11.15.03.40;  author avg;  state Exp;
branches ;
next     ;


desc
@
Препроцессор для C-компилятора.
@


1.3
log
@*** empty log message ***
@
text
@/*
 * $Log:        cpy.y,v $
 * Revision 1.2  87/01/11  16:04:46  avg
 * Версия ид ДЕМОС 1.3 & МНОС РЛ 1.2
 *
 */

/*
 * C препроцессор
 *
 *  Адаптирован для МНОС 1.1 СМ-4
 *    Антоновым В.Г. и Леонтьевым С.Е. ( ИПК Минавтопрома ) 18.05.1984.
 *  Модифицирован Антоновым В.Г. для обработки русских букв 19.05.1984.
 */

%term number stop DEFINED
%term EQ NE LE GE LS RS
%term ANDAND OROR
%left ','
%right '='
%right '?' ':'
%left OROR
%left ANDAND
%left '|' '^'
%left '&'
%binary EQ NE
%binary '<' '>' LE GE
%left LS RS
%left '+' '-'
%left '*' '/' '%'
%right '!' '~' UMINUS
%left '(' '.'
%{
/*NOXSTR*/
#ifndef lint
static char rcsid[] = "$Header: cpy.y,v 1.2 87/01/11 16:04:46 avg Exp $";
#endif
/*YESXSTR*/
%}
%%
S:      e stop  ={return($1);}


e:        e '*' e
		={$$ = $1 * $3;}
	| e '/' e
		={$$ = $1 / $3;}
	| e '%' e
		={$$ = $1 % $3;}
	| e '+' e
		={$$ = $1 + $3;}
	| e '-' e
		={$$ = $1 - $3;}
	| e LS e
		={$$ = $1 << $3;}
	| e RS e
		={$$ = $1 >> $3;}
	| e '<' e
		={$$ = $1 < $3;}
	| e '>' e
		={$$ = $1 > $3;}
	| e LE e
		={$$ = $1 <= $3;}
	| e GE e
		={$$ = $1 >= $3;}
	| e EQ e
		={$$ = $1 == $3;}
	| e NE e
		={$$ = $1 != $3;}
	| e '&' e
		={$$ = $1 & $3;}
	| e '^' e
		={$$ = $1 ^ $3;}
	| e '|' e
		={$$ = $1 | $3;}
	| e ANDAND e
		={$$ = $1 && $3;}
	| e OROR e
		={$$ = $1 || $3;}
	| e '?' e ':' e
		={$$ = $1 ? $3 : $5;}
	| e ',' e
		={$$ = $3;}
	| term
		={$$ = $1;}
term:
	  '-' term %prec UMINUS
		={$$ = -$2;}
	| '!' term
		={$$ = !$2;}
	| '~' term
		={$$ = ~$2;}
	| '(' e ')'
		={$$ = $2;}
	| DEFINED '(' number ')'
		={$$= $3;}
	| DEFINED number
		={$$ = $2;}
	| number
		={$$= $1;}
%%
# include "yylex.c"
@


1.2
log
@Версия ид ДЕМОС 1.3 & МНОС РЛ 1.2
@
text
@d2 4
a5 1
 * $Log$
d36 1
a36 1
static char rcsid[] = "$Header$";
d39 1
@


1.1
log
@Initial revision
@
text
@d1 12
a12 1
/*	@@(#)cpy.y	2.1	SCCS id keyword	*/
d30 6
d37 1
a37 1
S:	e stop	={return($1);}
d40 1
a40 1
e:	  e '*' e
@
