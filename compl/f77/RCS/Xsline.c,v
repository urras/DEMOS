@extern Xfd= -1; extern long Xs0;

char *Xsline( sh )
unsigned sh;
{
	static char Xslin[130];
	if( Xfd < 0  )
		{ return( "***" ); }
	lseek( Xfd, (long)sh+Xs0, 0 );
	Xslin[read( Xfd, Xslin+2, 127 )] = 0;
	return( Xslin+2 );
}

