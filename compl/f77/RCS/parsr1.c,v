@/* Вторая часть двухпроходного парсера */
#include "gramh1.h"
#ifndef YYMAXDEPTH
#define YYMAXDEPTH 150
#endif
#ifndef YYSTYPE
#define YYSTYPE int
#endif

YYSTYPE yyv[YYMAXDEPTH], ylval;
int ykod,nyyv,nyyvt,yacn;

yyparse()
{ int i;
  register YYSTYPE yyval =0, *yypvt;
  register int yym;
  YYSTYPE *yypv;
  yym = 0; yypv = yyv -1;
  next: ++yypv; *yypv = yyval;
  yym = getsy0();
  switch (yym)
  {case YRET: return(ykod);
   case YNEXT: yyval = ylval; yypv = yyv + nyyv; goto next;
   case YERR : yypv = yyv + nyyv;
		goto next;
   case YCASE: break;
   default: fatal1("Intermadiate code error %d",yym);
  }
  yypv = yyv + nyyv;
  yypvt = yyv + nyyvt;
  yym = yacn;
  yyval = yypv[1];
  switch(yym)
  {
#include "gram1.h"
  }
 goto next;
}
@
