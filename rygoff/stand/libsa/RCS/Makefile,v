1.6
log
@Добавлена работа с RCS при сборке
@
text
@#
# Makefile для библиотеки libsa
#
# $Log:	Makefile,v $
#
#
# Для версии исх. текстов без архивов RCS установите R=X
R=
RCS=RCS
COFLAGS=-l

INCLUDES=       -I.. -I/usr/include
CCFLAGS=        -O ${INCLUDES}

SRC=    SYS.c   ais.c   dvhp.c  fd.c    hk.c    \
	ht.c    lprf.c  pcopy.s prf.c   rk.c    \
	rl.c    rm.c    rp.c    rw.c    tm.c    \
	ts.c    uda.c   video.c xp.c


DRIVERS=        prf.o xp.o ht.o rk.o rm.o rl.o rp.o dvhp.o\
		hk.o ts.o tm.o rw.o fd.o uda.o video.o pcopy.o lprf.o

../libsa.a:     SYS.o ${DRIVERS} font1.o
		ar rv ../libsa.a $?
		ranlib ../libsa.a

font1.o: ../../dev/font1.cm ../../dev/font1.mm ../../dev/font1fix.c
	mm ../../dev/font1.mm < ../../dev/font1.cm  >font1.c
	cc -S font1.c
	mv font1.s f.s
	sed 's/data/text/' < f.s >font1.s
	cc -c font1.s ../../dev/font1fix.c
	ld -r -o f.o font1.o font1fix.o
	mv f.o font1.o
	-rm -f f.s font1.s font1.c font1fix.o

clear clean:
	-rm -f *.[ob]

CLRSRC: clear
	-rm -f *.c


SYS.o   : SYS.c
ais.o   : ais.c
dvhp.o  : dvhp.c
fd.o    : fd.c
hk.o    : hk.c
ht.o    : ht.c
lprf.o  : lprf.c
pcopy.o : pcopy.s
prf.o   : prf.c
rk.o    : rk.c
rl.o    : rl.c
rm.o    : rm.c
rp.o    : rp.c
rw.o    : rw.c
tm.o    : tm.c
ts.o    : ts.c
uda.o   : uda.c
video.o : video.c
xp.o    : xp.c



.DEFAULT:
	co ${COFLAGS} $@@,v


@


