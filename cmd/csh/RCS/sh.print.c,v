1.4
log
@Слита с версией alex-а из 4.2
@
text
@#ifndef lint
/*NOXSTR*/
static  char rcsid[] = "$Header: sh.print.c,v 1.3 86/06/03 15:04:10 dmitry Exp $";
/*YESXSTR*/
#endif  lint

/*
 * $Log:        sh.print.c,v $
 * Revision 1.3  86/06/03  15:04:10  dmitry
 * Убраны дурацкие ifdefы.
 *
 * Revision 1.2  86/06/02  19:52:27  dmitry
 * Слиянием с версией для МНОС 1.2 добавлены русские буквы.
 *
 * Revision 1.1  86/06/01  20:23:37  dmitry
 * DEMOS & MNOS 2 version
 *
 */

#include "sh.h"

p60ths(l)
	long l;
{

	l += 3;
	printf("%d.%d", (int) (l / 60), (int) ((l % 60) / 6));
}

psecs(l)
	long l;
{
	register int i;

	i = l / 3600;
	if (i) {
		printf("%d:", i);
		i = l % 3600;
		p2dig(i / 60);
		goto minsec;
	}
	i = l;
	printf("%d", i / 60);
minsec:
	i %= 60;
	printf(":");
	p2dig(i);
}

p2dig(i)
	register int i;
{

	printf("%d%d", i / 10, i % 10);
}

char    linbuf[128];
char    *linp = linbuf;

putchar(c)
	register int c;
{
	c &= 0377;
	if ( !ISQUOTED(c) && (c == 0177 || c < ' ' && c != '\t' && c != '\n')) {
		putchar('^');
		if (c == 0177)
			c = '?';
		else
			c |= 'A' - 1;
	}
	c = TRIMED(c);
	*linp++ = c;
	if (c == '\n' || linp >= &linbuf[sizeof linbuf - 2])
		flush();
}

draino()
{

	linp = linbuf;
}

flush()
{
	register int unit;
	int lmode = 0;

#include <sys/ioctl.h>

	if (linp == linbuf)
		return;
	if (haderr)
		unit = didfds ? 2 : SHDIAG;
	else
		unit = didfds ? 1 : SHOUT;
#ifdef TIOCLGET
	if (didfds==0 && ioctl(unit, TIOCLGET, &lmode)==0 &&
	    lmode & LFLUSHO) {
		lmode = LFLUSHO;
		ioctl(unit, TIOCLBIC, &lmode);
		write(unit, "\n", 1);
	}
#endif
	write(unit, linbuf, linp - linbuf);
	linp = linbuf;
}

plist(vp)
	register struct varent *vp;
{

	if (setintr)
		sigrelse(SIGINT);
	for (vp = vp->link; vp != 0; vp = vp->link) {
		int len = blklen(vp->vec);

		printf(vp->name);
		printf("\t");
		if (len != 1)
			putchar('(');
		blkpr(vp->vec);
		if (len != 1)
			putchar(')');
		printf("\n");
	}
	if (setintr)
		sigrelse(SIGINT);
}

putqchar(c)
register int c;
{
	putchar(QUOTED(c));
}
@


