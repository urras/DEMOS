1.2
log
@В BSD 2.9 и 4.2 разный порядок первых аргументов strout.
(см. исх. текст).
@
text
@#ifndef lint
/*NOXSTR*/
static  char rcsid[] = "$Header: printf.c,v 1.1 86/06/01 20:17:58 dmitry Exp $";
/*YESXSTR*/
#endif  lint

/*
 * $Log:        printf.c,v $
 * Revision 1.1  86/06/01  20:17:58  dmitry
 * DEMOS & MNOS 2 version
 *
 */

/*
 * Hacked "printf" which prints through putchar.
 * DONT USE WITH STDIO!
 */
printf(fmt, args)
char *fmt;
{
	_doprnt(fmt, &args, 0);
}

#ifdef pdp11
_strout(string, count, adjust, foo, fillch)
#else
_strout(count, string, adjust, foo, fillch)
#endif
register char *string;
register int count;
int adjust;
register struct { int a[6]; } *foo;
{

	if (foo != 0)
		abort();
	while (adjust < 0) {
		if (*string=='-' && fillch=='0') {
			putchar(*string++);
			count--;
		}
		putchar(fillch);
		adjust++;
	}
	while (--count>=0)
		putchar(*string++);
	while (adjust) {
		putchar(fillch);
		adjust--;
	}
}
@


