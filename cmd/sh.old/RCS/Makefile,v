text
@d2 1
a2 1
# Shkeleton for МНОС РЛ 1.2 ; last mod. : 13.02.85 @@VG
d4 2
a5 2
#  If you want to compile it for МНОС ИАЭ, change
#  VERSION from $(RL) to $(IAE)
d7 1
a7 1
VERSION=$(RL)
d12 10
d24 1
a24 1
SHELL  =sh
d28 6
a33 1
	print.o macro.o expand.o ctype.o msg.o msg1.o xs.o Rctype.o
d47 2
a48 1
$(OBJS) : defs.h mac.h mode.h name.h stak.h ctype.h Rsort.h
d50 1
a50 1
blok.o fault.o main.o stak.o:         brkincr.h
d52 1
a52 4
x.c xs.c : msg.c
	$(CC) -P $(VERSION) msg.c
	xstr msg.i
	-rm msg.i
d54 1
a54 3
msg.o : x.c
	$(CC) -c $(CFLAGS) x.c
	mv x.o msg.o
d56 2
a57 7
xs.o  : xs.c
	$(CC) -S $(CFLAGS) xs.c
	mv xs.s xy.s
	sed <xy.s >xs.s 's/.data/.text/'
	-rm xy.s
	$(CC) -c xs.s
	-rm xs.s
d59 4
a62 7
msg1.o : msg1.c
	$(CC) -S $(CFLAGS) msg1.c
	mv msg1.s xy.s
	sed <xy.s >msg1.s 's/.data/.text/'
	-rm xy.s
	$(CC) -c msg1.s
	-rm msg1.s
d64 1
a64 2
sh:     $(OBJS) Makefile
	$(CC) -o sh $(LDF) $(OBJS)
d66 7
a72 3
clear:
	-rm -f sh x.c xs.c
	-rm -f *.[obsi]
@


