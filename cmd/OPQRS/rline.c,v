@Против излишней ругани вставлен _exit(0).
@
text
@/*
**      Чтение строки из файла стандартного ввода и выдача ее
**      в файл стандартного вывода.
**
**      rline
**
**      Стандартное местоположение в системе:  /bin
**
**      Метод сборки:  cc -O -n -s
**
**      $Header: rline.c,v 1.2 88/09/20 17:22:27 korotaev Exp $
**      $Log:   rline.c,v $
 * Revision 1.2  88/09/20  17:22:27  korotaev
 * Исправлена программа на предмет работы с файлами (by AVG).
 *
 * Revision 1.1  88/05/03  19:32:31  root
 * Initial revision
 *
 * Revision 1.1  86/11/23  23:02:09  alex
 * ---
 *
**
*/

static  char Rcs_id[] = "$Header: rline.c,v 1.2 88/09/20 17:22:27 korotaev Exp $";

main ()
{
	     char  buf[512];
    register char *p;
    register int   l, ll;

    ll = l = read( 0, buf, 512 );
    if( ll == 512 ) ll--;
    buf[ll] = '\n';
    p = buf;
    while( *p++ != '\n' );
    write( 1, buf, p-buf );
    lseek( 0, (long)((p-buf)-l), 1 );
    _exit(0);
}
@


