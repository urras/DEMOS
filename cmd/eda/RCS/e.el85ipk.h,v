1.6
log
@Сделаны правки под новый драйвер без automerging.
@
text
@#ifndef lint
/*static char rcsid[] = "$Header: e.el85ipk.h,v 1.4 88/01/19 11:36:49 avg Exp $";*/
#endif

/*
 * $Log:        e.el85ipk.h,v $
 * Revision 1.4  88/01/19  11:36:49  avg
 * Правлен режим выделения - для новой версии драйвера.
 *
 * Revision 1.3  86/11/14  16:22:16  avg
 * Правки под новый exec.c.
 *
 * Revision 1.2  86/11/11  20:42:26  avg
 * Убран rcsid
 *
 * Revision 1.1  86/11/11  17:50:51  avg
 * Initial revision
 *
 */

/*
 * Экранный редактор eda  ver 2.4
 *   Автор: Антонов В.Г. ВМК МГУ, ИПК Минавтопрома
 *   Последняя модификация: 09.05.1986
 */

/* Параметры для имитатора терминала на Электронике-85 */

#define NLIN    24              /* Число строк */
#define SWID    80              /* Ширина экрана */
/* #define FRM     0               /* Курсор переходит за правый край экрана */
#define ROLL    0               /* Терминал работает со скроллингом */
#define BROLL   0               /* Для роллирования требуется поз. в 0,0 */

#define VS      "\033E"         /* Включить выделение */
#define VE      "\033F"         /* Выключить выделение */

/* Коды клавиатуры */

#define F_right "\0337"          /* Вправо */
#define F_left  "\0336"          /* Влево */
#define F_up    "\0339"          /* Вверх */
#define F_down  "\0338"          /* Вниз */
#define F_il    "\033Q"          /* Вставить строку */
#define F_dl    "\033P"          /* Удалить строку */
#define F_ic    "\033Y"          /* Вставить символ */
#define F_dc    "\033Z"          /* Удалить символ */
#define F_del   "\177"           /* Забой */
#define F_tab   "\011"           /* Табуляция вперед */
#define F_btab  "\033C"          /* Табуляция назад */
#define F_wtab  "\033B"          /* Табуляция вперед по словам */
#define F_bwtab "\033A"          /* Табуляция назад по словам */
#define F_exit  "\033S\033S"     /* Нормальный выход */
#define F_rew   "\001"           /* Перерисовка экрана */
#define F_na    "\012"           /* Ввод числового аргумента */
#define F_lnum  "\033T"          /* Выдать номер текущей строки */
#define F_cnum  "\033U"          /* Выдать номер текущей позиции в строке */
#define F_ret   "\010"           /* Установить в начало строки */
#define F_cr    "\015"          /* Возврат каретки */
#define F_aex   "\004\004\004\004" /* Аварийный выход */
#define F_inter "\003"          /* Прерывание */
#define F_flush "\027"          /* Отключение вывода на экран */
#define F_fsd   "\033K"         /* Установка подстроки поиска */
#define F_find  "\033L"         /* Поиск */
#define F_savel "\033I"         /* Запоминание строки в буфере */
#define Q_savel "\005"
#define F_incll "\033J"         /* Вставка буфера в текст */
#define Q_incll "\007"
#define F_wrtex "\033@@\033I"    /* Запись буфера в файл */
#define F_rdtex "\033@@\033J"    /* Заполнение буфера из файла */
#define F_clrtx "\033@@\027"     /* Очистка буфера */
#define F_end   "\033@@\0337"    /* В конец строки по непробелам */
#define F_beg   "\033@@\0336"    /* В начало строки по непробелам */
#define F_pgup  "\0330"         /* Страница вверх */
#define F_pgdown "\0331"        /* Страница вниз */
#define F_shell "\033R"         /* Вызов интерпретатора shell */
#define F_sins  "\033W"         /* Режим вставки */
#define Q_sins  "\033@@\0337"
#define F_sdel  "\033V"         /* Режим замены */
#define Q_sdel  "\033@@\0336"
#define F_tof   "\033H"         /* В начало файла */
#define F_eof   "\033G"         /* В конец файла */
#define F_cat   "\033X"         /* Склеивание/разрезание строк */
#define F_rpt   "\033@@\033@@"    /* Повторить пред. команду */
#define F_store "\033@@\033S"    /* Запомнить изменения */
#define F_pmode "\033O"         /* Редактирование программ */
#define F_tmode "\033N"         /* Редактирование текстов */
#define F_parup "\033@@\033H"    /* На параграф вверх */
#define F_pardown "\033@@\033G"  /* На параграф вниз */
/* #define F_format  "\012\033Q"     /* Форматирование абзаца*/
/* #define Q_format  "\012\033я"     /* Форматирование абзаца*/

#define F_mdef1   "\033M\0332"  /* Определить макро 1 */
#define F_mdef2   "\033M\0333"  /* Определить макро 2 */
#define F_mdef3   "\033M\0334"  /* Определить макро 3 */
#define F_mdef4   "\033M\0335"  /* Определить макро 4 */

#define F_m1      "\0332"       /* Выполнить макро 1 */
#define F_m2      "\0333"       /* Выполнить макро 2 */
#define F_m3      "\0334"       /* Выполнить макро 3 */
#define F_m4      "\0335"       /* Выполнить макро 4 */
#define FS_mdend  "\033M"       /* Символ конца определения макро*/

#define Mname1  '1'             /* Имя макро 1 */
#define Mname2  '2'             /* Имя макро 2 */
#define Mname3  '3'             /* Имя макро 3 */
#define Mname4  '4'             /* Имя макро 4 */

/* Управляющие символы терминала */

#define S_right "\020"          /* Вправо курсор */
#define S_left  "\010"          /* Влево курсор */
#define S_up    "\022"          /* Вверх курсор */
#define S_down  "\012"          /* Вниз курсор */
#define S_ret   "\015"          /* Возврат каретки */
#define S_cr    "\012\015"      /* Возврат каретки/перевод строки */
#define S_home  "\024"          /* В начало экрана */
#define S_rd    "\024\022"      /* Роллирование вниз */
#define S_clrl  "\025"          /* Очистка строки (курсор в начале строки) */
#define S_clrel "\025"          /* Очистка конца строки */
#define S_erase "\014"          /* Очистка экрана */
#define S_bell  "\007"          /* Звонок */
#define S_il    "\033T"         /* Вставка строки */
#define S_dl    "\033U"         /* Удаление строки */

#define S_pos   "\026  "        /* Последовательность прямого */
#define POSX    1               /* позиционирования           */
#define POSY    2               /*    см. pos.c               */
@


1.5
log
@1. Добавлены операции S_il и S_dl для новой версии драйвера
терминала  Э-85.
2. Добавлены альтернативные кодировки для режимов вст/зам и 
вставки/запоминания из буфера.
@
text
@d31 1
a31 1
#define FRM     0               /* Курсор переходит за правый край экрана */
@


