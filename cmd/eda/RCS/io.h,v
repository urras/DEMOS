/*
 * Упрощенный имитатор stdio.h
 */
#ifndef BufSIZE
#define BufSIZE 512

struct _iob {
	int     _chan;
	int     _flag;
	char   *_ptr;
	int     _cnt;
	char    _buf[BufSIZE];
};

#define FILE    struct _iob

#ifndef NULL
# define NULL   0
#endif

#define EOF     (-1)

FILE  *fopen();

#define _IOREAD   1
#define _IOWRITE  2
#define _IOEOF    4

#define NBUFS   3

FILE    _bufs[NBUFS];

#define stdout (&_bufs[0])
#endif BufSIZE
@


