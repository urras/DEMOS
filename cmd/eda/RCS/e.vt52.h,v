/* Параметры для терминала VT52 */

#define NLIN    24
#define SWID    80
#define SYMSTRT "\033=\033-"
#define SYMSTOP "\033>"
#define ROLL    0
#define BROLL 0

#define F_right "\033C"
#define F_left  "\033D"
#define F_up    "\033A"
#define F_down  "\033B"
#define F_il    "\033?r"
#define F_dl    "\033?x"
#define F_ic    "\033?v"
#define F_dc    "\033?t"
#define F_wtab  "\033?y\011"
#define F_bwtab "\033?y\033?p"
#define F_del   "\177"
#define F_tab   "\011"
#define F_btab  "\033?p"
#define F_exit  "\032\032"
#define F_rew   "\001"
#define F_na    "\033?M"
#define F_lnum  "\033?y\033?M"
#define F_cnum  "\033?w"
#define F_ret   "\012"
#define F_cr    "\015"
#define F_aex   "\004\004\004\004"
#define F_inter "\003"
#define F_fsd   "\025"
#define F_find  "\021"
#define F_savel "\005"
#define F_incll "\007"
#define F_wrtex "\033?y\005"
#define F_rdtex "\033?y\007"
#define F_clrtx "\033?y\027"
#define F_end   "\033?y\033C"
#define F_beg   "\033?y\033D"
#define F_pgup  "\033?y\033A"
#define F_pgdown "\033?y\033B"
#define F_shell "\023"
#define F_tof   "\002"
#define F_eof   "\006"
#define F_sins  "\033?y\033?v"
#define F_sdel  "\033?y\033?t"
#define F_cat   "\033?n"
#define F_rpt   "\033?y\033?y"
#define F_pmode "\033?s"
#define F_tmode "\033?q"
#define F_flush "\027"
#define F_parup "\033?y\033?x"
#define F_pardown "\033?y\033?r"
#define F_store "\033?y\033k"

#define F_mdef1 "\010\033P"
#define F_mdef2 "\010\033Q"
#define F_mdef3 "\010\033R"
#define F_m1    "\033P"
#define F_m2    "\033Q"
#define F_m3    "\033R"
#define FS_mdend "\010"
#define Mname1  '1'
#define Mname2  '2'
#define Mname3  '3'

#define S_right "\033C"
#define S_left  "\033D"
#define S_up    "\033A"
#define S_down  "\033B"
#define S_rd    "\033H\033I"
#define S_ret   "\015"
#define S_cr    "\012\015"
#define S_home  "\033H"
#define S_clrl  "\033K"
#define S_clrel "\033K"
#define S_erase "\033H\033J"
#define S_bell  "\007"

#define S_pos   "\033Y  "
#define POSX    2
#define POSY    3
