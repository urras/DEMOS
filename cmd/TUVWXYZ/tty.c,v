/*
 * Type tty name
 */
#include <ediag.h>

char    *ttyname();

main(argc, argv)
char **argv;
{
	register char *p;

	p = ttyname(0);
	if(argc==2 && !strcmp(argv[1], "-s"))
		;
	else
		printf("%s\n", (p? p: ediag("not a tty","не tty")));
	exit(p? 0: 1);
}
