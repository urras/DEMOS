#endif  /*IPK_XOVLY*/
TYPE    unsigned  SYMV;
TYPE    char  SYMFLG;
TYPE    char  OVTAG;

/* symbol table in a.out file */
struct symtab {
	char    symc[8];
	SYMFLG  symf;
	OVTAG   ovnumb;
	SYMV    symv;
};
#define SYMTABSIZ (sizeof (struct symtab))

#define SYMCHK 047
#define SYMTYPE(symflag) (( symflag>=041 || (symflag>=02 && symflag<=04))\
				?  ((symflag&07)>=3 ? DSYM : (symflag&07))\
				: NSYM\
			)
struct  ovhead {
	unsigned        max;
	unsigned        ov[7];
};
#ifdef  IPK_XOVLY
struct  xovhead {
	unsigned        xmax;
	unsigned        xov[31];
};
#endif  /*IPK_XOVLY*/
