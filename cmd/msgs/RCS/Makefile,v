R       =
COFLAGS =
CIFLAGS = -u -q
RCS     = RCS
DESTDIR =
CFLAGS=-O
all:  msgs
msgs: msgs.c msgs.h
	cc $(CFLAGS) -o msgs msgs.c -ltermlib
install:
	cp msgs ${DESTDIR}/usr/bin; strip /usr/bin/msgs
clean:
	-rm -f msgs
msgs.c :
	RCS=$(RCS) $(COFLAGS) co msgs.c
msgs.h :
	RCS=$(RCS) $(COFLAGS) co msgs.h
@


