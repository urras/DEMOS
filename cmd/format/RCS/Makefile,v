@#
#
# Makefile для программ форматирования
#
# $Log$
#

CFLAGS=-O
LDFLAGS=-s

all: wf

install: i_wf

i_wf: wf
	cp wf /etc/wf
	chmod 700 /etc/wf

wf: wf.c
	$(CC) -o wf $(LDFLAGS) $(CFLAGS) wf.c

clear clean:
	rm -f *.[ob] wf

CLRSRC: clear
	rm -f wf.c
@
