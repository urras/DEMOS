** $Log:	keys.c,v $
*/

#ifndef lint
char r_c_s_id[] = "$Header: keys.c,v 1.1 87/09/14 18:34:18 andrew Exp $";
#endif

#include <stdio.h>

static char *kw[] = {
    "auto",
    "break",
    "case",
    "char",
    "const",
    "continue",
    "default",
    "defined",
    "do",
    "double",
    "else",
    "enum",
    "extern",
    "float",
    "for",
    "goto",
    "if",
    "int",
    "long",
    "register",
    "return",
    "short",
    "signed",
    "sizeof",
    "static",
    "struct",
    "switch",
    "typedef",
    "union",
    "unsigned",
    "void",
    "volatile",
    "while",
    NULL
};

int     keyword (s)
char   *s;
{
    register    res;
    register char **ss;

    for (ss = kw; *ss && (res = strcmp (s, *ss)) > 0; ss++);
    return (res == 0);
}
@
