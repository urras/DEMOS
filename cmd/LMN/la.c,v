text
@/*
**
** Эта штука добавляется в исходный файл перед
** запуском rcs               Давидов 04.28.88
**
** $Header: la.c,v 1.3 88/09/19 19:28:18 root Exp $
** $Log:        la.c,v $
 * Revision 1.3  88/09/19  19:28:18  root
 * была опечатка.
 *
 * Revision 1.2  88/09/19  19:24:23  root
 * русишванификация.
 *
 * Revision 1.1  88/05/03  20:28:21  root
 * Initial revision
 *
*/

#include <ediag.h>

# ifndef        lint
/* NOXSTR  */
static char     Rcs_id[] = "$Header: la.c,v 1.3 88/09/19 19:28:18 root Exp $";
/* YESXSTR */
# endif         lint

/*
 * la: print the load average.
 */
main()
{
	double vec[3];

	if (loadav(vec) < 0) {
		perror("loadav: gldav");
		exit(1);
	}
	printf("%s %.02f %.02f %.02f\n",
		ediag( "load" , "загрузка" ),
		vec[0], vec[1], vec[2]);
	exit(0);
}
