/* $Header$ */

#ifndef SIGRETRO

#define SIGRETRO                                /* Can use this for cond code */

#ifndef SIG_HOLD

#define SIG_HOLD        (int (*)()) 3           /* Phony action to hold sig */
#define BADSIG          (int (*)()) -1          /* Return value on error */

#endif SIG_HOLD

#endif SIGRETRO

