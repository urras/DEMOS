DEST=    /usr/bin
CFLAGS=  -Ox
FP=
LFLAGS=  -n -s ${FP}
OBJS          = indent.o \
		io.o \
		lexi.o \
		parse.o \
		pr_comment.o

indent: $(OBJS)
	cc ${LFLAGS} -o indent ${OBJS}

clean:
	rm -f *.o *.b core indent

cp:     indent
	cp indent ${DEST}
	strip ${DEST}/indent

###
indent.o: in_globs.h in_codes.h
io.o: in_globs.h
lexi.o: in_globs.h in_codes.h
parse.o: in_globs.h in_codes.h
pr_comment.o: in_globs.h@


