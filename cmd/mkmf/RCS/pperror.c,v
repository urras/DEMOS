/*
 * pperror() writes a system error message to standard error output,
 * preceded by the name of the program and message.
 */
#include <stdio.h>

pperror(message)
	char *message;                  /* error message */
{
	extern char *PGN;               /* program name */

	fprintf(stderr, "%s: ", PGN);
	perror(message);
}
@
