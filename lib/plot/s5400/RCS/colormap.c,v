@/* $Header$
 * $Log$
 */

extern char _HEXtab[];

colormap( ncol, R, G, B )
{
	if( ncol & ~017 )
		return;
	R >>= 6;  R &= 017;
	G >>= 6;  G &= 017;
	B >>= 6;  B &= 017;

	printf( "JG%c%c%c%c0", _HEXtab[ncol],
			       _HEXtab[R], _HEXtab[G], _HEXtab[B] );
}
@
