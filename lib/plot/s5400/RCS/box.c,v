#include <stdio.h>

box(x0, y0, x1, y1)
short x0, y0, x1, y1 ;
{
    move( x0, y0 );
    printf( "FH" );
    cont( x1, y1 );
    printf( "FG" );
}

/* solid box */
rfill(x0, y0, x1, y1)
short x0, y0, x1, y1 ;
{
    move( x0, y0 );
    printf( "FHBG" );
    cont( x1, y1 );
    printf( "FG" );
}

/* заливка области */
sfill( x,y, newcolor, boundcolor )
{
	move( x,y);
	printf( "HI%XJK%X", newcolor, boundcolor );
}
/* ограничение :
   граница области ДОЛЖНА БЫТЬ ЗАМКНУТА И быть именно указанного цвета
   белый - 15 */
@


