# include <stdio.h>

char _HEXtab[] = "0123456789ABCDEF";

color( col )
unsigned col;
{
	if( col > 15 )                  /* Zero color IS a background color */
		col = 1+(col%15);
	printf( "HI%c", _HEXtab[col] );
}
@


