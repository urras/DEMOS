text
@/ $Header$
/ $Log$
/ error = mknod(name, mode, addr);
/	  char *name;

.globl	_mknod, cerror

_mknod:
	mov	r5,-(sp)
	mov	sp,r5
	mov	4(r5),0f
	mov	6(r5),0f+2
	mov	8(r5),0f+4
	sys	0; 9f
	bec	1f
	jmp	cerror
1:
	clr	r0
	mov	(sp)+,r5
	rts	pc
.data
9:
	sys	mknod; 0:..; ..; ..
@
