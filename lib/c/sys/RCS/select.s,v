.globl  _select
.globl  cerror

_select:
	mov     r5,-(sp)
	mov     sp,r5
	mov     4.(r5),r0
	mov     6.(r5),0f
	mov     8.(r5),0f+2
	mov     10.(r5),0f+4
	mov     12.(r5),0f+6
	sys     local; 9f
	bec     1f
	jmp     cerror
