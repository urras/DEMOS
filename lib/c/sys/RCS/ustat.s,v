head     1.1;
access   ;
symbols  ;
locks    ;
comment  @/@;


1.1
date     89.05.23.13.20.23;  author abs;  state Exp;
branches ;
next     ;


desc
@
системный вызов получения статистики про свободное место в
файловой системе
@


1.1
log
@Initial revision
@
text
@/ $Header$
/ $Log: ustat.s,v $
/
/ error = ustat(dev, buf);
/         struct ustat *buf;

.globl  _ustat
.globl  cerror

_ustat:
	mov     r5,-(sp)
	mov     sp,r5
	mov     4(r5),r0
	mov     6(r5),0f
	sys     local; 9f
	bec     1f
	jmp     cerror
1:
	clr     r0
	mov     (sp)+,r5
	rts     pc
.data
9:
	sys     ustat; 0:..
@
