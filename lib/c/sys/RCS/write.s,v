1.2
log
@Аргументы в стеке
@
text
@/ $Header: write.s,v 1.1 88/04/01 17:44:38 andrew Exp $
/ $Log:	write.s,v $
/ Revision 1.1  88/04/01  17:44:38  andrew
/ Initial revision
/ 
/ nwritten = write(fildes, buffer, nbytes);
/	  char *buffer;
/	  unsigned nbytes;
/ nwritten == -1 means error

.globl	_write
.globl	cerror

_write:
	mov	r5,-(sp)
	mov	sp,r5
	mov	4(r5),r0
	mov	8(r5),-(sp)
	mov	6(r5),-(sp)
	sys	write+200
	bec	1f
	cmp	(sp)+, (sp)+
	jmp	cerror
1:
	cmp	(sp)+, (sp)+
	mov	(sp)+,r5
	rts	pc
@


