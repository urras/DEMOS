1.1
log
@Initial revision
@
text
@/ $Header$
/ $Log$
/ error = truncate(name, lenght);
/         char *name;
/         off_t lenght;                 /* это long */

.globl  _truncate
.globl  cerror

_truncate:
	mov     r5,-(sp)
	mov     sp,r5
	mov     4(r5),0f
	mov     6(r5),0f+2
	mov     8(r5),0f+4
	sys     local; 9f
	bec     1f
	jmp     cerror
1:
	clr     r0
	mov     (sp)+,r5
	rts     pc
.data
9:
	sys     truncate; 0:..; ..; ..
@
