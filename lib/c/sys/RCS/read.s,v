1.2
log
@Аргументы в стеке.
@
text
@/ $Header: read.s,v 1.1 88/04/01 17:43:10 andrew Exp $
/ $Log:	read.s,v $
/ Revision 1.1  88/04/01  17:43:10  andrew
/ Initial revision
/ 
/ nread = read(fildes, buffer, nbytes);
/	  char *buffer;
/	  unsigned nbytes;
/ nread ==0 means eof; nread == -1 means error

.globl	_read
.globl	cerror

_read:
	mov	r5,-(sp)
	mov	sp,r5
	mov	4(r5),r0
	mov	8(r5),-(sp)
	mov	6(r5),-(sp)
	sys	read+200
	bec	1f
	cmp	(sp)+, (sp)+
	jmp	cerror
1:
	cmp	(sp)+, (sp)+
	mov	(sp)+,r5
	rts	pc
@


