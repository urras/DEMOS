head     1.2;
access   ;
symbols  ;
locks    ;
comment  @/@;


1.2
date     89.01.31.14.37.11;  author abs;  state Exp;
branches ;
next     1.1;

1.1
date     89.01.27.14.15.58;  author abs;  state Exp;
branches ;
next     ;


desc
@
удаление каталога.
@


1.2
log
@у лок. сисвызовов должно быть
sys local; 
а не 	sys 0;
@
text
@/ $Header: rmdir.s,v 1.1 89/01/27 14:15:58 abs Exp $
/ $Log: rmdir.s,v $
/Revision 1.1  89/01/27  14:15:58  abs
/Initial revision
/
/ Revision 1.1  89/01/27  14:06:45  abs
/ Initial revision
/
/
/ error = rmdir(name);
/         char *name;

.globl  _rmdir
.globl  cerror

_rmdir:
	mov     r5,-(sp)
	mov     sp,r5
	mov     4(r5),0f
	sys     local; 9f
	bec     1f
	jmp     cerror
1:
	clr     r0
	mov     (sp)+,r5
	rts     pc
.data
9:
	sys     rmdir; 0:..
@


1.1
log
@Initial revision
@
text
@d1 1
a1 1
/ $Header: rmdir.s,v 1.1 89/01/27 14:06:45 abs Exp $
d3 3
d20 1
a20 1
	sys     0; 9f
@
