GET=co
GFLAGS=-q
I=/usr/include/
IS=/usr/include/sys/
S=/usr/include/stdio.h

.DEFAULT:
	$(GET) $(GFLAGS) $@@

.s.o:
	as -V -o $@@ /usr/include/sys.s $<
	ld -r -x $@@ -o a.out.lib
	mv a.out.lib $@@

.c.o:
	cc -O -c $<
	ld -r -x $@@ -o a.out.lib
	mv a.out.lib $@@

