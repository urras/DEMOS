% === DPOTST === %  12.10.88 VERSION 3
LOCALS LDP
  EQUALS COIST=&FF70,COOST=&FF74
  EQUALS READY=&80,ITENB=&40

PROGRAM DPINIT()
  @'COOST'=.BIC.'ITENB'
  @'COIST'=.BIC.'ITENB'
  RETURN ,

PROGRAM DPOTST()
  RETURN R0(@'COOST').BIC.'&FFFF-READY'
PROGRAM DPO()
  @'COOST+2':=R0
  RETURN ,

PROGRAM DPMTST()
  RETURN R0(@'COIST').BIC.'&FFFF-READY'
PROGRAM DPM()
  R0=R0:(@'COIST+2').BIC.&FF00
  RETURN ,

PROGRAM DPEND()
  @'COIST'=.BIS.'ITENB'
  RETURN ,
